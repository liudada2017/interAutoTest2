[
  {
    "description": "模拟一个不带数据的post请求",
    "request": {
      "uri": "/api/post/demo",
      "method": "post"
    },
    "response": {
      "text": "hello no params post"
    }
  },
  {
    "description": "模拟一个带数据forms的post请求",
    "request": {
      "uri": "/api/post/forms/demo",
      "method": "post",
      "forms": {
        "name": "dada",
        "age": "18"
      }
    },
    "response": {
      "text": "hello forms post"
    }
  },
  {
    "description": "模拟一个带数据json的post请求",
    "request": {
      "uri": "/api/post/json/demo",
      "method": "post",
      "json": {
        "name": "dada",
        "age": "18"
      }
    },
    "response": {
      "status": 201,
      "json": {
        "text": "hello json post"
      }
    }
  },
  {
    "description": "模拟一个带数据带cookies的post请求",
    "request": {
      "uri": "/api/post/json/cookies/demo",
      "method": "post",
      "json": {
        "name": "dada",
        "age": "18"
      },
      "cookies": {
        "login": "true"
      }
    },
    "response": {
      "status": 201,
      "text": "hello json params post cookies"
    }
  },
  {
    "description": "模拟一个带数据带cookies带headers的post请求",
    "request": {
      "uri": "/api/post/json/cookies/headers/demo",
      "method": "post",
      "cookies": {
        "login": "true"
      },
      "json": {
        "name": "dada",
        "age": "18"
      },
      "headers": {
        "content-type": "application/json"
      }
    },
    "response": {
      "status":201,
      "text": "hello json params post cookies headers"
    }
  }
]